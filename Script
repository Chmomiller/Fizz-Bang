#Every whole number between 1 and 100
for i in range (1,101):
    #If it's divisible with a remainder of zero by both 3 and 5
    if i % 3 == 0 and i % 5 == 0:
        print "FizzBang";
    elif i % 3 == 0:
        print "Fizz";
    elif i % 5 == 0:
        print "Bang";
    #For every other number just print the number
    else:
        print i;
